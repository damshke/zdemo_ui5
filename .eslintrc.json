{
    "env": {
        "browser": true,
        "es2021": true
    },        
    "rules": {
        "key-spacing": 2,
        "no-console": 2,
        "keyword-spacing": 2,
        "eqeqeq": 2,
        "no-dupe-else-if": 2,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty": 2,
        "no-fallthrough": 2,
        "no-invalid-regexp": 2,
        "no-promise-executor-return": 2,
        "no-unsafe-negation": 2,
        "no-unused-vars": 2,
        "no-use-before-define": 2,
        "use-isnan": 2,
        "valid-typeof": 2,
        "block-scoped-var": 2,
        "camelcase": 2,        
        "no-bitwise": 2,
        "no-else-return": 2,
        "no-eq-null": 2,
        "no-undef-init": 2,        
        "arrow-spacing": 2,
        "func-call-spacing": 2,
        "no-mixed-spaces-and-tabs": 2,
        "no-multi-spaces": 2,
        "no-multiple-empty-lines": 2,        
        "no-whitespace-before-property": 2,
        "object-curly-spacing": 2,
        "space-before-blocks": 2,
        "space-before-function-paren": 2,
        "space-in-parens": 2,
        "space-infix-ops": 2,
        "space-unary-ops": 2,
        "switch-colon-spacing": 2,
        "template-curly-spacing": 2
    }
}
